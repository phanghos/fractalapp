<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false" android:state_activated="false">
        <shape>
            <solid android:color="#fff" />
        </shape>
    </item>
    <item android:state_pressed="true" android:state_activated="false">
        <shape>
            <solid android:color="#F5F5F5" />
        </shape>
    </item>
    <item android:state_pressed="false" android:state_activated="true">
        <shape>
            <solid android:color="#F5F5F5" />
        </shape>
    </item>
</selector>